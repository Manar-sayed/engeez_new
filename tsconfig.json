{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/page.tsx", "components/FetchDaleelItems.jsx", "components/Navbar/Navbar.jsx", "app/(pages)/daleelcom/page.jsx", "components/GlobalSlider.jsx", "app/blog/[departmentId].tsx", "app/blog/page.tsx", "app/blog/[id].js"],
  "exclude": ["node_modules"]
}
